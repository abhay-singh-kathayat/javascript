const questionOptionsCheckedValue=question.querySelectorAll('.quiz-option input:checked')[0].value;const questionOptionsCheckedLabel=question.querySelectorAll('.quiz-option input:checked')[0].parentElement;const questionOptionsCorrectValue=question.querySelectorAll('.quiz-option input[data-correct="true"]')[0].value;const questionOptionsCorrectLabel=question.querySelectorAll('.quiz-option input[data-correct="true"]')[0].parentElement;if(questionOptionsChecked.length===0){unanswered++;}else if(questionOptionsCheckedValue===questionOptionsCorrectValue){correctAnswers++;questionOptionsCheckedLabel.classList.add('correct');}else{wrongAnswers++;questionOptionsCheckedLabel.classList.add('wrong');questionOptionsCorrectLabel.classList.add('correct');}
questionOptions.forEach(option=>{option.disabled=true;});});result=`Correct Answers: ${correctAnswers}<br>Wrong Answers: ${wrongAnswers}<br>Unanswered: ${unanswered}`;quizResultText.style.display='block';quizResultText.innerHTML=result;}
